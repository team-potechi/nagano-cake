<div class="container">
  <%= flash[:notice] %>
  <h5 class="text-center", style="background-color:#eee; width:120px; margin:30px 0 30px 115px;"><b>商品編集</b></h5>
  <div class="row">
    <div class="col-8 m-auto">
      <div class="col-10">
        <%= form_with model:@item, url: admin_item_path(@item.id) do |f| %>
          <%= render 'form',f: f, genres: @genres  %>
        <% end %>
      </div>
    </div>
  </div>
</div>